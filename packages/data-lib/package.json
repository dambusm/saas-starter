{
  "private": true,
  "name": "@next-hasura-boilerplate/data-lib",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "generate": "yarn introspect && graphql-codegen --config src/data-sources/hasura/graphql/codegen.yml",
    "introspect": "source '.env' && graphqurl ${HASURA_GRAPHQL_URL} -H \"x-hasura-admin-secret: ${HASURA_ADMIN_SECRET}\" --introspect > src/data-sources/hasura/graphql/autogenerated/schema.graphql"
  },
  "dependencies": {
    "graphql": "15.4.0",
    "graphql-request": "3.4.0",
    "graphql-tag": "2.12.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-compatibility": "2.0.1",
    "@graphql-codegen/typescript-graphql-request": "3.0.0",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "graphqurl": "1.0.1"
  },
  "peerDependencies": {
    "typescript": "*"
  }
}
